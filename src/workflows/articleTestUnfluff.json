{
  "Comment": "A Article fetch-/extract test with node-unfluff",
  "StartAt": "FetchArticle",
  "States": {
    "FetchArticle": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:eu-west-1:855433257886:function:fetchArticle",
      "Next": "ExtractArticle"
    },
    "ExtractArticle": {
      "Type": "Parallel",
      "Next": "MergeJSONs",
      "Branches": [
        {
          "StartAt": "ExtractArticleTitle",
          "States": {
            "ExtractArticleTitle": {
              "Type": "Task",
              "Resource": "arn:aws:lambda:eu-west-1:855433257886:function:extractArticleTitle",
              "End": true
            }
          }
        },
        {
          "StartAt": "ExtractArticleDate",
          "States": {
            "ExtractArticleDate": {
              "Type": "Task",
              "Resource": "arn:aws:lambda:eu-west-1:855433257886:function:extractArticleDate",
              "End": true
            }
          }
        },
        {
          "StartAt": "ExtractArticleText",
          "States": {
            "ExtractArticleText": {
              "Type": "Task",
              "Resource": "arn:aws:lambda:eu-west-1:855433257886:function:extractArticleText",
              "End": true
            }
          }
        }
      ]
    },
    "MergeJSONs": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:eu-west-1:855433257886:function:mergeJSONs",
      "End": true
    }
  }
}
